{
  "manifest_version": 3,
  "name": "URL Checker",
  "version": "1.0",
  "description": "Checks if the current URL is in the list of saved URLs.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "logo.png"
    }
  },
  "icons": {
    "128": "logo.png"
  }
}
